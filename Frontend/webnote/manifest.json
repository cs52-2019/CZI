{
  "manifest_version": 2,
  "name": "Webnote",
  "description" : "Annotate papers on the web!",
  "version": "2.2",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
     "scripts": [
       "background.js",
       "index.js"
     ],
     "persistent": false
  },
  "permissions": [
    "identity"
  ],
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "905397597566-pdidsbko18a6alqpiimc8greid1pvr5s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}

